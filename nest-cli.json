{
  "monorepo": true,
  "root": "apps/nest-typeorm",
  "sourceRoot": "apps/nest-typeorm/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest-typeorm/tsconfig.app.json"
  },
  "projects": {
    "nest-typeorm": {
      "type": "application",
      "root": "apps/nest-typeorm",
      "entryFile": "main",
      "sourceRoot": "apps/nest-typeorm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-typeorm/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    }
  }
}
